{
  "deno.enable": true,
  "deno.lint": true,
  "deno.unstable": true,
  "editor.tokenColorCustomizations": {
    "[Omni Owl (Minimal Italics)]": {
      "textMateRules": [
        {
          "scope": "entity.name.function",
          "settings": {
            "foreground": "#67E480"
          }
        }
      ]
    }
  },
  "ocaml.sandbox": {
    "kind": "opam",
    "switch": "4.12.0+flambda+nnpchecker"
  },
  "files.watcherExclude": {
    "**/target": true
  }
}
